{
	"name": "eelly/eelly-sdk-php",
	"description": "eelly sdk for php",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Eelly",
			"homepage": "https://www.eelly.com"
		}
	],
	"require": {
		"php": "^7.1.3",
		"shadon/framework": "^v0.2",
		"shadon/oauth2-client": "^v0.1.0"
	},
	"autoload": {
		"psr-4": {
			"Eelly\\DocsBundle\\":"src/DocsBundle",
			"Eelly\\DTO\\":"src/DTO",
			"Eelly\\Events\\":"src/Events",
			"Eelly\\Exception\\":"src/Exception",
			"Eelly\\SDK\\":"src/SDK"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
